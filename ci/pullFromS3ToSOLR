echo "Hello I am pull from S3 to SOLR Job"

echo "I am running on "  $(hostname)

echo "My Index is ${GO_JOB_RUN_INDEX}"

mkdir -p s3-to-solr-track

cat > s3-to-solr-track/S3ToSOLRTrackTable.csv <<EOF

This is S3 TO SOLR Tracking Table

Generated on $(hostname)

Job Index ${GO_JOB_RUN_INDEX} / ${GO_JOB_RUN_COUNT}

At $(date)

EOF
